<!DOCTYPE html>
<html lang="{{lang}}">

<head>
  <title>KTH | {{title}}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="{{description}}">
  <link rel="shortcut icon" id="favicon" href="{{proxyPrefix}}/static/icon/favicon">

  {{#if canonicalUrl}}
  <link href="{{canonicalUrl}}" rel="canonical">
  {{/if}}

  {{prefixStyle '/static/kth-style/css/kth-bootstrap.css' 'styles' 'screen'}}
  {{prefixStyle '/assets/fonts.css' 'styles' 'screen'}}
  {{prefixStyle '/static/app.css' 'styles' 'screen'}}

  {{{render 'styles'}}}

  {{prefixScript '/static/vendor.js' 'head-scripts'}}
  {{prefixScript '/static/browserConfig' 'head-scripts'}}

  {{{render 'head-scripts'}}}

  <script src="{{toolbarUrl}}"></script>

</head>

<body>
  {{> kthHeader }}
  <div class="kth-content">
    <div class="row">
        {{{body}}}
    </div>
  </div>
  {{{render 'scripts'}}}
</body>

</html>