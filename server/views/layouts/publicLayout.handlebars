<!DOCTYPE html>
<html>
  <head>
    <title>KTH | {{title}}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{description}}">
    <link rel="shortcut icon" id="favicon" href="//www.kth.se/img/icon/favicon.ico">

    {{#if canonicalUrl}}
      <link href="{{canonicalUrl}}" rel="canonical">
    {{/if}}
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
    {{prefixStyle '/static/kth-style/css/kth-bootstrap.min.css' 'styles' 'screen'}}
    {{prefixStyle '/static/css/kursinfo-web.css' 'styles' 'screen'}}

    {{{render 'styles'}}}

    {{prefixScript '/static/js/vendor.js' 'head-scripts'}}
    {{prefixScript '/static/browserConfig' 'head-scripts'}}
    {{#extend 'head-scripts'}}
      <script src="//www.kth.se/social/toolbar/widget.js"></script>
      <!-- analytics start -->
      {{{blocks.analytics}}}
      <!-- analytics end -->
    {{/extend}}

    {{#if debug}}
      {{prefixScript '/static/js/lib/firebug-lite/build/firebug-lite.js' 'head-scripts'}}
    {{/if}}

    {{{render 'head-scripts'}}}
  </head>
  <body>
      {{> kthHeader }}
      <div class="container main">
        <div class="row">
          {{{body}}}
        </div>
      </div>
      {{> kthFooter }}
    {{{render 'scripts'}}}
  </body>
</html>