<script>{{{compressedData}}}</script>

{{prefixScript '/static/app.js' 'scripts'}}<!---->
{{! <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script> }}

<!--h1>Kurinformation - Sprint 1</h1-->
<div id='kth-kursinfo'>
  <div key='kursinfo-container' className='kursinfo-main-page col'>

    <h1>Alla mina kurser</h1>
    <div>
      <span className='Header--Button'>
        <a href='/kursinfoadmin/kurser/kurs/{{courseCode}}' class='link-back'>Tillbaka till admin</a>
      </span>
    </div>

    {{#if error}}
      <p>Error: {{error}}</p>
    {{else}}
      <div id='api-data'>

        <ul>
          {{#each html}}
            <li>{{this}}</li>
          {{/each}}
        </ul>

      </div>
    {{/if}}
  </div>
</div>