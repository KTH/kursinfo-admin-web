    "gulp": "^3.9.1",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "kth-node-build-commons": "^4.1.4",
    "merge-stream": "^2.0.0",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "pre-push": "^0.1.1"
  },
  "engines": {
    "node": "10.14.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
