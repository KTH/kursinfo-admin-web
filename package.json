{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursinfo-admin-web",
  "version": "3.0.0",
  "automaticPublish": "true",
  "description": "Administration tool for teachers to edit introduction text and start sida presenting the entrance to other microservices like course memo och course history",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-admin-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:pack-dev": "bash ./build.sh docker",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "docker:start-dev": "npm run docker:build && npm run docker:run",
    "//": "Gulp installs css and javascript",
    "prepare": "husky install",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "test": "npx cross-env NODE_ENV=test jest test --passWithNoTests",
    "test-file": "cross-env NODE_ENV=test jest test ./test/CourseDescriptionEditor --watch",
    "test-watch": "jest --watch",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build-gulp": "cross-env NODE_ENV=development gulp watch",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@babel/runtime": "^7.23.5",
    "@kth/api-call": "^4.0.40",
    "@kth/kth-node-passport-oidc": "^5.0.5",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^9.2.0",
    "@kth/kth-reactstrap": "^0.4.78",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.2.1",
    "@kth/server": "^4.0.4",
    "@kth/session": "^3.0.9",
    "applicationinsights": "^2.9.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "kth-node-access-log": "^0.2.9",
    "kth-node-configuration": "^2.0.13",
    "kth-node-express-routing": "^2.0.16",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.2.0",
    "kth-style": "^9.0.11",
    "lint-staged": "^15.2.0",
    "locale": "^0.1.0",
    "lodash": "^4.17.21",
    "os": "^0.1.2",
    "passport": "^0.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.22.1",
    "reactstrap": "^9.2.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@kth/eslint-config-kth": "^3.1.0",
    "@kth/kth-ckeditor-build": "^4.0.13",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "decache": "^4.6.2",
    "eslint": "^8.56.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "mini-css-extract-plugin": "^2.8.0",
    "nodemon": "^3.0.2",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^3.1.0",
    "prettier-eslint-cli": "^8.0.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "glob-parent": "6.0.2"
  },
  "engines": {
    "node": "18"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write"
    ]
  }
}
