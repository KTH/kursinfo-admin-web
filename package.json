{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursinfo-admin-web",
  "version": "3.0.0",
  "description": "Administration tool for teachers to edit introduction text and start sida presenting the entrance to other microservices like course memo och course history",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-admin-web.git"
  },
  "scripts": {
    "//": "Gulp installs css and javascript",
    "preinstall": "npx npm-force-resolutions",
    "test": "jest test",
    "test-watch": "jest --watch",
    "vendor": "parcel build ./public/js/vendor.js",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && gulp build && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev": "NODE_ENV=development rimraf ./dist && gulp build && parcel build ./public/js/app/app.jsx && parcel build ./public/js/vendor.js --public-url /kursinfoadmin/kurser/kurs/static && parcel watch ./public/js/app/app.jsx --public-url /kursinfoadmin/kurser/kurs/static",
    "build-gulp": "cross-env NODE_ENV=development gulp watch",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
  "dependencies": {
    "@azure/storage-blob": "^12.2.1",
    "@kth/kth-node-passport-oidc": "^1.0.38",
    "applicationinsights": "^1.8.10",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "component-registry": "^1.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-handlebars": "^5.2.1",
    "jquery": "^3.5.1",
    "kth-node-access-log": "^0.2.2",
    "kth-node-api-call": "^3.0.13",
    "kth-node-configuration": "^2.0.3",
    "kth-node-express-routing": "^2.0.5",
    "kth-node-i18n": "^1.0.5",
    "kth-node-log": "^3.1.3",
    "kth-node-monitor": "^1.1.9",
    "kth-node-redis": "^3.1.19",
    "kth-node-response": "https://github.com/KTH/kth-node-response#v1.0.0",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^5.1.12",
    "kth-style": "^5.0.30",
    "locale": "^0.1.0",
    "lodash": "^4.17.20",
    "mobx": "^5.15.6",
    "mobx-react": "^5.4.4",
    "passport": "^0.4.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-csv": "^2.0.1",
    "react-dom": "^16.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "safe-utils": "^1.0.1",
    "sharp": "^0.23.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@kth/eslint-config-kth": "^1.0.10",
    "@kth/kth-ckeditor-build": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "gulp": "^4.0.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.32.8"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "engines": {
    "node": "12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  }
}
