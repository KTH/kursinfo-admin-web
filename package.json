{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursinfo-admin-web",
  "version": "3.0.0",
  "automaticPublish": "true",
  "description": "Administration tool for teachers to edit introduction text and start sida presenting the entrance to other microservices like course memo och course history",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-admin-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:pack-dev": "bash ./build.sh docker",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "docker:start-dev": "npm run docker:build && npm run docker:run",
    "//": "Gulp installs css and javascript",
    "prepare": "husky install",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "postinstall": "npm run mini-css-extract-plugin && bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'",
    "mini-css-extract-plugin": "yarn add mini-css-extract-plugin --no-lockfile --ignore-scripts",
    "test": "cross-env NODE_ENV=test jest test --passWithNoTests",
    "test-file": "cross-env NODE_ENV=test jest test ./test/CourseDescriptionEditor --watch",
    "test-watch": "jest --watch",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build-gulp": "cross-env NODE_ENV=development gulp watch",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.12.0",
    "@babel/runtime": "^7.18.9",
    "@kth/api-call": "^4.0.40",
    "@kth/kth-node-passport-oidc": "^4.1.7",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^7.0.15",
    "@kth/kth-reactstrap": "^0.4.54",
    "@kth/log": "^4.0.4",
    "@kth/monitor": "^3.0.29",
    "@kth/server": "^4.0.2",
    "@kth/session": "^3.0.4",
    "applicationinsights": "^2.3.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "browser-image-compression": "^2.0.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "kth-node-access-log": "^0.2.8",
    "kth-node-configuration": "^2.0.13",
    "kth-node-express-routing": "^2.0.14",
    "kth-node-i18n": "^1.0.14",
    "kth-node-redis": "^3.1.42",
    "kth-style": "^7.0.27",
    "locale": "^0.1.0",
    "lodash": "^4.17.21",
    "os": "^0.1.2",
    "passport": "^0.6.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.3.0",
    "reactstrap": "^9.1.5",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@kth/eslint-config-kth": "^3.0.10",
    "@kth/kth-ckeditor-build": "^4.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.2.2",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "decache": "^4.6.1",
    "eslint": "^8.27.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^28.1.2",
    "jest-extended": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^6.0.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": "16"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  }
}
