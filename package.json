{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursinfo-admin-web",
  "version": "2.0.0",
  "description": "Template project for a node express web server",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-admin-web.git"
  },
  "scripts": {
    "//": "Gulp installs css and javascript",
    "test": "mocha test/unit/*.js",
    "codecheck": "standard",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production gulp build",
    "build-dev": "cross-env NODE_ENV=development gulp watch",
    "build-react": "NODE_ENV=development rm -rf ./dist && gulp build && parcel build ./public/js/app/app.jsx &&  parcel build ./public/js/vendor.js && parcel watch ./public/js/app/app.jsx",
    "inferno-dev": "cross-env NODE_ENV=development gulp inferno",
    "start": "NODE_ENV=production node app.js",
    "start-react": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-react\" \"nodemon --ignore dist/ --ignore public/ app.js\"",
    "start-dev": "cross-env NODE_ENV=development gulp build && cross-env NODE_ENV=development concurrently --kill-others \"nodemon --ignore dist/**/* app.js\" \"gulp watch\""
  },
  "pre-push": [
    "codecheck",
    "test"
  ],
  "dependencies": {
    "@azure/storage-blob": "^10.4.0",
    "@babel/polyfill": "^7.4.4",
    "applicationinsights": "^1.4.1",
    "babel-eslint": "^10.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "co": "^4.6.0",
    "component-registry": "^0.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.4",
    "express-handlebars": "^3.1.0",
    "gulp-install": "^1.1.0",
    "jquery": "^3.4.1",
    "knockout": ">=3.5.0",
    "knockout.viewmodel": "git+https://github.com/KTH/knockout.viewmodel.git#v2.0.3",
    "kth-client-logging": "git+https://github.com/KTH/kth-client-logging.git#v1.0.0",
    "kth-node-access-log": "^0.2.0",
    "kth-node-api-call": "^1.3.0",
    "kth-node-configuration": "^1.4.0",
    "kth-node-express-routing": "^0.3.1",
    "kth-node-i18n": "^1.0.2",
    "kth-node-ldap": "^2.4.0",
    "kth-node-log": "^3.0.0",
    "kth-node-monitor": "^0.1.19",
    "kth-node-passport-cas": "^3.1.9",
    "kth-node-redis": "^1.2.0",
    "kth-node-response": "https://github.com/KTH/kth-node-response#v1.0.0",
    "kth-node-server": "^3.0.3",
    "kth-node-session": "^1.0.0",
    "kth-node-web-common": "^3.0.6",
    "kth-style": "^3.2.15",
    "kth-style-inferno-bootstrap": "^0.1.23",
    "locale": "^0.1.0",
    "mobx": "^5.13.0",
    "mobx-react": "^5.4.3",
    "passport": "^0.3.2",
    "query-string": "^5.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^7.1.0",
    "safe-utils": "^0.1.1",
    "sharp": "^0.23.0"
  },
  "devDependencies": {
    "@kth/kth-ckeditor-build": "^1.4.1",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "chai": "^4.0.2",
    "concurrently": "^2.1.0",
    "cross-env": "^2.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "gulp": "^3.9.1",
    "husky": "^0.14.0",
    "kth-node-build-commons": "^3.0.3",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0",
    "mockery": "^2.0.0",
    "nock": "^9.0.13",
    "node-mocks-http": "^1.7.6",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.3",
    "pre-push": "^0.1.1",
    "standard": "^7.1.1",
    "zombie": "^6.1.4"
  },
  "engines": {
    "node": "8.12.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
