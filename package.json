  },
  "overrides": {
    "glob-parent": "6.0.2"
  },
  "engines": {
    "node": "20"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write"
    ]
  }
