{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursinfo-admin-web",
  "version": "3.0.0",
  "automaticPublish": "true",
  "description": "Administration tool for teachers to edit introduction text and start sida presenting the entrance to other microservices like course memo och course history",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-admin-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:pack-dev": "bash ./build.sh docker",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "docker:start-dev": "npm run docker:build && npm run docker:run",
    "//": "Gulp installs css and javascript",
    "prepare": "husky install",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\"",
    "test": "npx cross-env NODE_ENV=test jest test --passWithNoTests",
    "test-file": "cross-env NODE_ENV=test jest test ./test/CourseDescriptionEditor --watch",
    "test-watch": "jest --watch",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build-gulp": "cross-env NODE_ENV=development gulp watch",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.25.0",
    "@babel/runtime": "^7.25.9",
    "@kth/api-call": "^4.1.0",
    "@kth/appinsights": "^0.4.0",
    "@kth/cortina-block": "^7.0.0",
    "@kth/kth-node-passport-oidc": "^5.2.0",
    "@kth/kth-node-response": "^1.0.8",
    "@kth/kth-node-web-common": "^9.3.1",
    "@kth/kth-reactstrap": "^1.0.2",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.3.1",
    "@kth/server": "^4.1.0",
    "@kth/session": "^3.0.9",
    "@kth/style": "^1.7.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.1",
    "kth-node-access-log": "^0.2.10",
    "kth-node-configuration": "^2.1.0",
    "kth-node-express-routing": "^2.2.0",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.3.0",
    "kth-style": "^10.4.0",
    "lint-staged": "^15.2.10",
    "locale": "^0.1.0",
    "@kth/om-kursen-ladok-client": "^1.3.12",
    "os": "^0.1.2",
    "passport": "^0.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0",
    "reactstrap": "^9.2.3",
    "safe-utils": "^1.0.1"
